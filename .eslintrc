{
    "extends": "eslint:recommended",
    "rules": {
        "camelcase": 2,
        "curly": 2,
        "eqeqeq": 2,
        "no-alert": 2,
        "no-bitwise": 1,
        "eol-last": 2
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "globals": {
        "$": false
    }
}
